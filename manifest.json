{
    "manifest_version": 3,
    "name": "Google Calendar Agenda Guard",
    "version": "1.0",
    "permissions": ["identity", "identity.email", "scripting"],
    "oauth2": {
        "client_id": "oauth_client",
        "scopes": ["https://www.googleapis.com/auth/calendar"]
    },
    "background": {
        "server_worker": "background.js"
    }
}